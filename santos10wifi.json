{
    "__extends__": "samsung.common",
    "recovery": {
        "img": "imgs/synced_recovery.img",
        "stock": "imgs/recovery.img"
    },
    "__config__": {
        "target": {
            "arch": "x86",
            "mount": {
                "data": {
                    "mount": "/data",
                    "fs": "ext4",
                    "dev": "/dev/block/mmcblk0p9",
                    "size": 12654215168
                },
                "cache": {
                    "mount": "/cache",
                    "fs": "ext4",
                    "dev": "/dev/block/mmcblk0p6",
                    "size": 367001600
                },
                "boot": {
                    "mount": "/boot",
                    "fs": "emmc",
                    "dev": "/dev/block/mmcblk0p10",
                    "size": 20971520
                },
                "recovery": {
                    "mount": "/recovery",
                    "fs": "emmc",
                    "dev": "/dev/block/mmcblk0p11",
                    "size": 20971520
                },
                "system": {
                    "mount": "/system",
                    "fs": "ext4",
                    "dev": "/dev/block/mmcblk0p8",
                    "size": 2474639360
                }
            }
        }
    },
    "boot": {
        "img": "imgs/synced_boot.img"
    }
}
